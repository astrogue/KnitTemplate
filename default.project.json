{
  "name": "Roblox",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Game": {
        "$className": "Folder",
        "Shared": {
          "$className": "Folder",
          "$path": "src/Game/Shared"
        }
      },
      "Knit": {
        "$path": "lib/Knit/src"
      },
      "Assets": {
        "$ignoreUnknownInstances": true,
        "$path": "lib/Assets"
      }
    },
    "ServerScriptService": {
      "$className": "ServerScriptService",
      "Game": {
        "$className": "Folder",
        "$path": "src/Game/Server",
        "Modules": {
          "$className": "Folder",
          "RemoteComponent": {
            "$path": "lib/RemoteComponent/default.project.json"
          }
        }
      }
    },
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "Game": {
          "$className": "Folder",
          "$path": "src/Game/Client",
          "Modules": {
            "$className": "Folder",
            "RemoteComponent": {
              "$path": "lib/RemoteComponent/default.project.json"
            }
          }
        }
      }
    }
  }
}